{
  "name": "ui-public",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "normalize.css": "8.0.1",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-router-dom": "5.0.1",
    "react-scripts": "3.1.1",
    "styled-components": "4.3.2"
  },
  "devDependencies": {
    "prettier-eslint": "9.0.0",
    "prettier-stylelint": "0.4.2",
    "stylelint": "15.10.1",
    "stylelint-config-standard": "18.3.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-order": "3.0.1",
    "stylelint-processor-styled-components": "1.8.0"
  },
  "scripts": {
    "start": "PORT=3000 REACT_APP_API_HOST=http://localhost:5000 react-scripts start",
    "build": "GENERATE_SOURCEMAP=false react-scripts build",
    "test": "REACT_APP_API_HOST=http://localhost:5000 CI=true react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint ./src/"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babelMacros": {
    "styledComponents": {
      "pure": true
    }
  }
}
